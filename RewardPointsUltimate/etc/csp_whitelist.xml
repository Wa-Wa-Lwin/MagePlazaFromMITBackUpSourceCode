<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="mp-reward-points-ultimate-addthis-script" type="host">*.addthis.com</value>
                <value id="mp-reward-points-ultimate-moatads-script" type="host">*.moatads.com</value>
                <value id="mp-reward-points-ultimate-addthisedge-script" type="host">*.addthisedge.com</value>
                <value id="mp-reward-points-ultimate-fb-script" type="host">*.facebook.net</value>
                <value id="mp-reward-points-ultimate-twitter-script" type="host">*.twitter.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="mp-reward-points-ultimate-addthis-frame" type="host">*.addthis.com</value>
                <value id="mp-reward-points-ultimate-fb-frame" type="host">*.facebook.com</value>
                <value id="mp-reward-points-ultimate-twitter-frame" type="host">*.twitter.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="data" type="host">data:</value>
                <value id="mp-reward-points-ultimate-addthisedge-img" type="host">*.addthisedge.com</value>
                <value id="mp-reward-points-ultimate-twitter-img" type="host">*.twitter.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
