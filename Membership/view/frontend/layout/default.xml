<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="top.links">
            <block class="Mageplaza\Membership\Block\Link" name="membership-link" after="wish-list-link">
                <arguments>
                    <argument name="sortOrder" xsi:type="number">50</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="footer_links">
            <block class="Magento\Framework\View\Element\Html\Link" name="mpmembership.footer_links" template="Mageplaza_Membership::view/footer-links.phtml" after="-"/>
        </referenceBlock>
    </body>
</page>
