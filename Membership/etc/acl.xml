<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::marketing">
                    <resource id="Mageplaza_Membership::mpmembership" title="Membership" translate="title" sortOrder="110">
                        <resource id="Mageplaza_Membership::membership" title="Manage Membership" translate="title" sortOrder="10"/>
                        <resource id="Mageplaza_Membership::member" title="Manage Members" translate="title" sortOrder="20"/>
                        <resource id="Mageplaza_Membership::history" title="Transactions" translate="title" sortOrder="30"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Mageplaza_Membership::configuration" title="Mageplaza Membership"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
