<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mpmembership>
            <general>
                <enabled>1</enabled>
                <default_group>1</default_group>
                <allow_override>1</allow_override>
                <allow_upgrade>1</allow_upgrade>
                <upgrading_cost>2</upgrading_cost>
                <show_on_toplink>1</show_on_toplink>
                <change_membership_when>1</change_membership_when>
            </general>
            <page>
                <enabled>1</enabled>
                <page_link_area>1,2</page_link_area>
                <page_route>membership</page_route>
            </page>
        </mpmembership>
    </default>
</config>
